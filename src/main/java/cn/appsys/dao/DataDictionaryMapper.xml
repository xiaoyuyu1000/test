<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- namespace表示命名空间 -->
<mapper namespace="cn.appsys.dao.DataDictionaryMapper">

    <select id="find"  parameterType="int" resultType="dataDictionary">
        select * from data_dictionary where id=#{userType}
    </select>
    <select id="getDataDictionaryList" resultType="DataDictionary">
        select * from  data_dictionary
        <trim prefix="where" prefixOverrides="and | or">
            <if test="typeCode != null">
                and typeCode = #{typeCode}
            </if>
        </trim>
        <!-- order by creationDate DESC  -->
    </select>
</mapper>